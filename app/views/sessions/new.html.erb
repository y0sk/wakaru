<h1>ログイン</h1>
<%= form_for(:session, url: login_path) do |f| %>
     <div class="form-group">
        <%= f.label :email, 'Email' %>
        <%= f.email_field :email %>
      </div>
       <div class="form-group">
        <%= f.label :password, 'Password' %>
        <%= f.password_field :password %>
      </div>

      <%= f.submit 'ログイン' %>
<% end %>
<p>新規登録はこちら→ <%= link_to '新規登録', signup_path %></p>
